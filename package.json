{
  "name": "qpet-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve:cli": "vue-cli-service serve",
    "build:cli": "vue-cli-service build",
    "lint:cli": "vue-cli-service lint",
    "start:dev:server": "NODE_ENV=development webpack-dev-server --inline --hot --config webpack.config.dev.js",
    "build:dev": "NODE_ENV=development webpack --config webpack.config.dev.js",
    "build:prod": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "dev": "node server",
    "start": "NODE_ENV=production node server",
    "build": "rm -rf dist;npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production webpack --config serverBuildUtils/webpack.client.config.js --progress --hide-modules",
    "build:server": "NODE_ENV=production webpack --config serverBuildUtils/webpack.server.config.js --progress --hide-modules"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "firebase": "^5.4.2",
    "lru-cache": "^4.1.3",
    "normalize.css": "^8.0.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17"
  },
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/lru-cache": "^4.1.1",
    "@types/node": "^10.9.4",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-standard": "^3.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "chokidar": "^2.0.4",
    "file-loader": "^2.0.0",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "postcss-preset-env": "^5.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "svg-sprite-loader": "^3.9.2",
    "ts-loader": "^5.1.1",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.0.3",
    "vue-loader": "^15.4.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-hot-middleware": "^2.23.1",
    "webpack-merge": "^4.1.4",
    "webpack-serve": "^2.0.2"
  }
}
